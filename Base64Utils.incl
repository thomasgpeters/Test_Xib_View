local fn ImageToBase64String( image as ImageRef ) as CFStringRef'~'1CFDataRef   imageDataCFStringRef imageStringimageData = fn ImageTIFFRepresentation( image )imageString = fn DataBase64EncodedString( imageData, NSDataBase64Encoding64CharacterLineLength )end fn = imageStringlocal fn Base64StringToImage( string as CFStringRef ) as ImageRef'~'1CFDataRef  imageDataImageRef   image = NULLimageData = fn DataWithBase64EncodedString( string, NSDataBase64DecodingIgnoreUnknownCharacters )image = fn ImageWithData( imageData )end fn = image// local fn TestConversion// '~'1// 	ImageRef    image, outImage// 	CFStringRef imageStr// 	// 	image = fn ImageNamed( @"flower.jpg" )// 	imageStr = fn ImageToBase64String( image )//   NSLog( @"%@", imageStr )  // This will will take some time to print the string to NSLog, but it provides an idea of the encoding// 	outImage = fn Base64StringToImage( imageStr )// 	ImageViewSetImage( _imageView1, outImage )// end fn